<div class="form-booking">
  <%= simple_form_for [rocket, booking] do |f| %>
    <%= f.input :booking_start_date, as: :date, html5: true %>
    <%= f.input :booking_end_date, as: :date, html5: true  %>
    <%= f.input :group_size, class: 'form-booking-font' %>
    <%= f.submit class: 'btn btn-ghost-booking' %>
    <% end %>
</div>
